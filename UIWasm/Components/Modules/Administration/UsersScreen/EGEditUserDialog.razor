    <FluentNumberField TValue="long" @bind-Value="Content.Id" Disabled="true">User id:</FluentNumberField>
    <FluentTextField @bind-Value="Content.UserName">Username: </FluentTextField>
    <FluentTextField @bind-Value="Content.FirstName">First name: </FluentTextField>
    <FluentTextField @bind-Value="Content.LastName">Last name: </FluentTextField>
</FluentDialogBody>
